package com.greatlearning.balancedBrackets;
import java.util.*;

class bracketsCheck {
	public static void main(String[] args) {
		String expr = "([{}])";
		 
        // Function call
        if (areBracketsBalanced(expr))
            System.out.println("Balanced ");
        else
            System.out.println("Not Balanced ");
	}
}